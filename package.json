        "url": "https://github.com/phaserjs/template-vite-ts/issues"
    },
    "homepage": "https://github.com/phaserjs/template-vite-ts#readme",
    "scripts": {
        "dev": "node scripts/log.js dev & vite --config scripts/vite/config.dev.mjs",
        "build": "node scripts/log.js build & vite build --config scripts/vite/config.prod.mjs",
        "dev-nolog": "vite --config scripts/vite/config.dev.mjs",
        "build-nolog": "vite build --config scripts/vite/config.prod.mjs",
        "lint": "eslint .",
        "test": "vitest",
        "test:ui": "vitest --ui"
    },
    "devDependencies": {
