        "@vitest/ui": "^4.0.13",
        "eslint": "^9.39.1",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import-x": "^4.16.1",
        "jsdom": "^27.2.0",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.47.0",
        "vite": "^6.3.1",
        "vitest": "^4.0.13"
